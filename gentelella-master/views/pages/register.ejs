<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Registration</title>
    <!-- Bootstrap -->
    <link href="assets/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="assets/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="assets/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- Animate.css -->
    <link href="assets/vendors/animate.css/animate.min.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="assets/build/css/custom.min.css" rel="stylesheet">
</head>

<body class="login">
    <div>
        <a class="hiddenanchor" id="signup"></a>
        <a class="hiddenanchor" id="signin"></a>

        <div class="login_wrapper">
            <div class="animate form login_form">
                <section class="login_content">
                    <!START OF FORM>
                        <form method="post" action="DebugCreate">
                            <h1>Registration</h1>

                            <div>
                                <!INPUT OF Firstname>
                                    <input type="text" class="form-control" placeholder="Username" name="Fname"
                                        required="" />
                            </div>
                            <div>
                                <!INPUT FOR Lastname>
                                    <input type="password" class="form-control" placeholder="Password" name="Lname"
                                        required="" />
                            </div>
                            <div>
                                <!INPUT FOR Email Address>
                                <input id="email" class="form-control" type="email" name="email" placeholder="email@email.com">
                            </div>
                            <div>
                                    <!INPUT FOR Contact Number>
                                    <input type="number" id="contact" name="contact" required="required" class="form-control" placeholder="Cellphone or Contact Number" maxlength="11">
                                </div>
                                <br>
                            <div>

                                <button type="submit" class="btn btn-success">Register</button>
                            </div>

                            <div class="clearfix"></div>

                            <div class="separator">
                                <div class="clearfix"></div>
                                <br />

                                <div>

                                    <h1><i class="fa fa-hand-o-right"></i> AccredIT</h1>
                                    <!-- Replace Icon with Group Logo -->

                                </div>
                            </div>
                        </form>
                </section>
            </div>
        </div>
    </div>
</body>

</html>